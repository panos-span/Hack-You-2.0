+import javax.swing.*;
+import java.awt.*;
+import java.awt.event.ActionEvent;
+import java.awt.event.ActionListener;
+
+public class QuizFrame implements ActionListener {
+
+    JFrame frame;
+    JLabel label = new JLabel();
+    JRadioButton[] radioButtons = new JRadioButton[4];
+    JButton submit = new JButton("Submit");
+    ButtonGroup bg = new ButtonGroup();
+    private final ImageIcon icon = new ImageIcon("src/main/resources/question_mark.png");
+
+    public QuizFrame() {
+        frame.setTitle("Question"); //setTitle of frame
+        //Για να μην κλείνει το παράθυρο μέχρι να απαντήσει ο παίκτης
+        frame.setDefaultCloseOperation(JFrame.DO_NOTHING_ON_CLOSE);
+        frame.setResizable(false);
+        frame.setSize(600, 600);
+        frame.setVisible(true);
+        frame.setLayout(null);
+        frame.setIconImage(icon.getImage());
+
+        frame.add(label);
+        for (JRadioButton button : radioButtons) {
+            frame.add(button);
+            bg.add(button);
+        }
+
+        frame.add(submit);
+
+    }
+
+    /**
+     * Μέθοδος δημιουργίας Κουμπιών
+     */
+    public void setButton(JButton button, int y) {
+        button.setBounds(600, y, 100,50);
+        button.setFocusable(false);
+        button.addActionListener(this);
+        button.setHorizontalAlignment(JButton.CENTER);
+        button.setFont(new Font("Arial", Font.ITALIC,20));
+    }
+
+    @Override
+    public void actionPerformed(ActionEvent e) {
+
+    }
+}
